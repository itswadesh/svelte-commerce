# Litekart Specific Settings
PUBLIC_LITEKART_API_URL=https://api.litekart.in # Litekart Backend API URL
PUBLIC_LITEKART_DOMAIN=demo.litekart.in # The store domain of Litekart that you wish to serve. Change this to your own domain or any subdomain of Litekart

# MedusaJS Specific Settings
PUBLIC_MEDUSAJS_API_URL=https://medusa-api.litekart.in

# Core Configuration
PUBLIC_CORE_CURRENCY_CODE=USD
PUBLIC_CORE_CURRENCY_SYMBOL=$
PUBLIC_CORE_DIMENTION_UNITS=cm
PUBLIC_CORE_WEIGHT_UNIT=g

# S3 Settings
SECRET_S3_ACCESS_KEY=YOUR_SECRET_S3_ACCESS_KEY
SECRET_S3_BUCKET_NAME=YOUR_SECRET_S3_BUCKET_NAME
SECRET_S3_REGION=YOUR_SECRET_S3_REGION
SECRET_S3_SECRET_KEY=YOUR_SECRET_S3_SECRET_KEY

# Logs Collector
SECRET_SENTRY_DSN=

